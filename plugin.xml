<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-posplugin" version="1.0.0"
	xmlns="http://apache.org/cordova/ns/plugins/1.0"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<name>posplugin</name>
	<js-module name="posplugin" src="www/posplugin.js">
		<clobbers target="posplugin" />
	</js-module>
	<platform name="android">
		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="posplugin">
				<param name="android-package" value="cordova.plugin.posplugin.posplugin" />
			</feature>
		</config-file>
		<config-file parent="/*" target="AndroidManifest.xml" />
		<source-file src="src/android/posplugin.java" target-dir="src/cordova/plugin/posplugin/posplugin" />
	</platform>
	<platform name="ios">
		<config-file parent="/*" target="config.xml">
			<feature name="posplugin">
				<param name="ios-package" value="posplugin" />
			</feature>
		</config-file>
		<source-file src="src/ios/posplugin.m" />
	</platform>
</plugin>